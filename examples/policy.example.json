{
  "version": 2,
  "workspace": {
    "default_strategy": "create",
    "branch_prefix": "task",
    "base_branch": "main"
  },
  "docs": {
    "authority_mode": "workspace_active",
    "seed_mode": "copy_from_repo_on_start",
    "stale_warning_seconds": 900,
    "api": {
      "request_timeout_seconds": 3,
      "workspace_endpoints": [
        {
          "name": "workspace-metawsm",
          "base_url": "http://127.0.0.1:8787",
          "web_url": "http://127.0.0.1:8787",
          "repo": "metawsm",
          "workspace": "metawsm-001"
        }
      ],
      "repo_endpoints": [
        {
          "name": "repo-metawsm",
          "base_url": "http://127.0.0.1:8790",
          "web_url": "http://127.0.0.1:8790",
          "repo": "metawsm"
        }
      ]
    }
  },
  "tmux": {
    "session_pattern": "{agent}-{workspace}"
  },
  "execution": {
    "step_retries": 1
  },
  "health": {
    "idle_seconds": 300,
    "activity_stalled_seconds": 900,
    "progress_stalled_seconds": 1200
  },
  "close": {
    "require_clean_git": true
  },
  "agent_profiles": [
    {
      "name": "codex-default",
      "runner": "codex",
      "base_prompt": "You are the implementation agent for this ticket run. You start in the kickoff-selected documentation repository, and that repository contains the docmgr ttmp root. Use docmgr there to manage tasks/docs/changelog and keep a diary while you work. If blocked, write .metawsm/guidance-request.json. When done, write .metawsm/implementation-complete.json and .metawsm/validation-result.json with status=passed and done_criteria matching the run brief.",
      "skills": [
        "docmgr",
        "diary"
      ],
      "runner_options": {
        "full_auto": true
      }
    }
  ],
  "agents": [
    {
      "name": "agent",
      "profile": "codex-default"
    }
  ]
}
